# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Set default charset
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# 4 space indentation
[*.xml]
indent_style = space
indent_size = 2
